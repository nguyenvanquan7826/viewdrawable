<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextViewDrawable">
        <attr name="icon_position" format="enum">
            <enum name="left" value="0" />
            <enum name="top" value="1" />
            <enum name="right" value="2" />
            <enum name="bottom" value="3" />
        </attr>
        <attr name="colorFilter" format="color" />
        <attr name="drawableLeftCompat" format="reference" />
        <attr name="drawableRightCompat" format="reference" />
        <attr name="drawableTopCompat" format="reference" />
        <attr name="drawableBottomCompat" format="reference" />
        <attr name="font_name" format="string" />
    </declare-styleable>
</resources>